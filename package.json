    "babel-preset-stage-3": "^6.24.1",
    "bootstrap": "^4.3.1",
    "concurrently": "^4.1.0",
    "emotion": "^9.2.12",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-emotion": "^9.2.12",
    "react-redux": "^5.1.1",
